<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zlw.bean">
	<class name="com.zlw.bean.User" table="user_info">
		<id name="user_id" type="java.lang.Integer">
			<generator class="identity"></generator>
		</id>
		<property name="username" type="java.lang.String" />
		<property name="paw" type="java.lang.String" />
		<property name="state" type="java.lang.String" />
		<property name="date" type="java.lang.String" />
		<property name="address" type="java.lang.String" />
		<property name="phonenum" type="java.lang.String" />
		<property name="email" type="java.lang.String" />
		<property name="user_photo" type="java.lang.String" />

		<!-- <set name="suqareSet" inverse="true" lazy="false"> -->
